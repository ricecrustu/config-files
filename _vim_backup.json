{   
 "vim.autoSwitchInputMethod.defaultIM": "2057",
    "vim.changeWordIncludesWhitespace": true,
    "vim.cursorStylePerMode.replace": "underline",
    "vim.easymotion": true,
    "vim.easymotionMarkerForegroundColorOneChar": "#80ffea",
    "vim.foldfix": true,
     "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "#454158",
    "vim.highlightedyank.duration": 250,
     "vim.smartRelativeLine": true,
     "extensions.experimental.affinity": {
        "vscodevim.vim": 1,
     }
    "vim.handleKeys": {
        "<C-a>": false,
        // "<C-f>": false,
        // "<C-n>": false,
        "<C-w>": false
    },
    "vim.hlsearch": true,
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "j",
                "k"
            ]
        }
    ],
    "vim.leader": "\\",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "g",
                "p",
                "d"
            ],
            "commands": [
                "editor.action.peekDefinition"
            ]
        },
        {
            "before": [
                "g",
                "h"
            ],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        {
            "before": [
                "g",
                "p",
                "i"
            ],
            "commands": [
                "editor.action.peekImplementation"
            ]
        },
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "editor.action.referenceSearch.trigger"
            ]
        },
        {
            "before": [
                "g",
                "q"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "g",
                "t"
            ],
            "commands": [
                "editor.action.goToTypeDefinition"
            ]
        },
        {
            "before": [
                "g",
                "p",
                "t"
            ],
            "commands": [
                "editor.action.peekTypeDefinition"
            ]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "whichkey.show"
            ]
        },
        {
            "before": [
                "<leader>",
                "/"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "after": [
                "^"
            ],
            "before": [
                "H",
            ]
        },
        {
            "after": [
                "$"
            ],
            "before": [
                "L"
            ]
        },
        {
            "after": [
                "<leader>",
                "<leader>",
                "/"
            ],
            "before": [
                "<leader>",
                "f"
            ]
        },
        {
            "before": [
                "n",
            ],
            "after": [
                "n",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "N",
            ],
            "after": [
                "N",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "o",
            ],
            "after": [
                "o",
                "<esc>"
            ]
        },
        {
            "before": [
                "O",
            ],
            "after": [
                "O",
                "<esc>"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "commands": [
                "editor.debug.action.toggleBreakpoint"
            ]
        },
        {
            "before": [
                "<leader>",
                "a"
            ],
            "commands": [
                "debug.enableOrDisableBreakpoint"
            ]
        },
        {
            "before": [
                "<leader>",
                "c"
            ],
            "commands": [
                "workbench.action.debug.continue"
            ]
        },
        {
            "before": [
                "<leader>",
                "o"
            ],
            "commands": [
                "workbench.action.debug.stepOut"
            ]
        },
        {
            "before": [
                "<leader>",
                "s"
            ],
            "commands": [
                "workbench.action.debug.stepOver"
            ]
        },
        {
            "before": [
                "<leader>",
                "i"
            ],
            "commands": [
                "workbench.action.debug.stepInto"
            ]
        },
        {
            "before": [
                "<Tab>"
            ],
            "commands": [
                ":tabn"
            ]
        },
        {
            "before": [
                "<Shift+tab>",
            ],
            "commands": [
                ":tabp"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "m"
            ],
            "commands": [
                ":marks"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "j"
            ],
            "commands": [
                ":ju"
            ]
        },
        {
            "before": [
                "<leader>",
                "d",
                "m"
            ],
            "commands": [
                ":delm!"
            ]
        }
    ],
    "vim.operatorPendingModeKeyBindings": [
        {
            "before": [
                "L",
            ],
            "after": [
                "$"
            ]
        },
        {
            "after": [
                "^"
            ],
            "before": [
                "H"
            ]
        }
    ],
    "vim.useCtrlKeys": true,
    "vim.useSystemClipboard": true,
    "vim.visualModeKeyBindings": [
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "v"
            ]
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "after": [
                "$"
            ],
            "before": [
                "L"
            ]
        },
        {
            "after": [
                "^"
            ],
            "before": [
                "H"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "whichkey.show"
            ]
        },
        {
            // By default, "p" in visual mode will copy whatever text was replaced. To prevent this, after pasting, select the newly replaced text and copy it.
            "before": [
                "p"
            ],
            "after": [
                "p",
                "g",
                "v",
                "y"
            ]
        },
    ]
}
